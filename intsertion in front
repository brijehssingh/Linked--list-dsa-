#include <bits/stdc++.h>
using namespace std;

class Node{
     
     public:
      
      int data ;
      Node* next; 
      
     
     Node( int data){
         this->data = data; 
         next  = NULL;
     }
     
     
    static void printValues(Node* head){
         
         while(head!=nullptr){
             
             cout<<head->data<<"->";
              head = head->next;
               
             
         }
         
     }
     
    
};


int main() {
	// your code goes here
	
	
	
	
  Node* head; 
  head = nullptr;
    
     std::vector<int >values{1,2,3,4,5,6,7,8} ;
      
      
          for( int i =0 ;i<values.size() ;i++){
              
              if(!head){
                  
                  head = new Node(values[i]);
              }
              
              else{
                  
                  Node* temp = new Node(values[i]);
                   
                    temp->next = head; 
                    head = temp; 
                    
                  
              }
              
          }
          
           
       
      
       Node::printValues(head);
        
        
  
     

}
